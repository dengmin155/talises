<SIMULATION>
  <DIM>2</DIM> 
  <FILENAME>0.000_1.bin</FILENAME>
  <GUESS_2D>exp( -0.25*((x-x_0)/sigma_x)^2 )*exp( -0.25*((y-y_0)/sigma_y)^2 )</GUESS_2D>
  <CONSTANTS>
    <N>1</N>
    <x_0>-5</x_0>
    <y_0>-5</y_0>
    <sigma_x>1</sigma_x>
    <sigma_y>1</sigma_y>
  </CONSTANTS>
    <NX>256</NX>
    <NY>256</NY>
    <XMIN>-10</XMIN><XMAX>10</XMAX>
    <YMIN>-10</YMIN><YMAX>10</YMAX>
</SIMULATION>