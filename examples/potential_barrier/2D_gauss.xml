<SIMULATION>
  <DIM>2</DIM> 
  <FILENAME>0.000_1.bin</FILENAME>
  <PSI_REAL_2D>exp( -0.25*((x-x_0)/sigma_x)^2 )*exp( -0.25*((y-y_0)/sigma_y)^2 )*cos(k*y)</PSI_REAL_2D>
  <PSI_IMAG_2D>exp( -0.25*((x-x_0)/sigma_x)^2 )*exp( -0.25*((y-y_0)/sigma_y)^2 )*sin(k*y)</PSI_IMAG_2D>
  <CONSTANTS>
    <N>1</N>
    <x_0>0</x_0>
    <y_0>-7.5e-6</y_0>
    <sigma_x>3e-6</sigma_x>
    <sigma_y>0.5e-6</sigma_y>
    <k>4e6</k>
  </CONSTANTS>
   <ALGORITHM>
    <NX>256</NX>
    <NY>256</NY>
    <XMIN>-10e-6</XMIN><XMAX>10e-6</XMAX>
    <YMIN>-10e-6</YMIN><YMAX>10e-6</YMAX>
    </ALGORITHM>
</SIMULATION>