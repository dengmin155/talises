<SIMULATION>
  <DIM>2</DIM> 
  <FILENAME>0.000_1.bin</FILENAME>
  <PSI_REAL_2D>exp( -0.25*((x-x_0)/sigma_x)^2 )*exp( -0.25*((y-y_0)/sigma_y)^2 )</PSI_REAL_2D>
  <PSI_IMAG_2D>0</PSI_IMAG_2D>
  <L>1e-6</L>
  <CONSTANTS>
    <N>1</N>
    <x_0>-5</x_0>
    <y_0>0</y_0>
    <sigma_x>0.5</sigma_x>
    <sigma_y>2</sigma_y>
  </CONSTANTS>
    <NX>256</NX>
    <NY>256</NY>
    <XMIN>-10</XMIN><XMAX>10</XMAX>
    <YMIN>-10</YMIN><YMAX>10</YMAX>
</SIMULATION>