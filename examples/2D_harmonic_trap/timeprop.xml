<SIMULATION>
  <N_THREADS>4</N_THREADS>
  <DIM>2</DIM>
  <INTERNAL_DIM>2</INTERNAL_DIM>
  <FILENAME>0.000_1.bin</FILENAME>
  <FILENAME_2>0.000_2.bin</FILENAME_2>
  <L>1e-6</L>
  <T>1e-6</T>
  <M>1.44466899e-25</M>
  <CONSTANTS>
    <m>1.44466899e-25</m>
    <hbar>1.054571817e-34</hbar>
    <f_R>2500</f_R>
    <f_HO>100</f_HO>
    <k>6e6</k>
  </CONSTANTS>
  <SEQUENCE>
      <interact Nk="50" dt="0.2" output_freq="packed" pn_freq="each"
V_11_real="0" V_11_imag="0" 			V_12_real="2*pi*f_R/2*cos(k*y)" V_12_imag="-2*pi*f_R/2*sin(k*y)"
						V_22_real="-hbar*k^2/2/m" V_22_imag="0"
>100</interact>
    <freeprop Nk="500" dt="2" output_freq="packed" pn_freq="each"
V_11_real="m/hbar/2*(2*pi*f_HO)^2*(x^2+y^2)" V_11_imag="0" V_22_real="m/hbar/2*(2*pi*f_HO)^2*(x^2+y^2)" V_22_imag="0"
>10000</freeprop> 
      <interact Nk="50" dt="0.2" output_freq="packed" pn_freq="each"
V_11_real="0" V_11_imag="0" 			V_12_real="2*pi*f_R/2*cos(k*y)" V_12_imag="-2*pi*f_R/2*sin(k*y)"
						V_22_real="-hbar*k^2/2/m" V_22_imag="0"
>100</interact>
  </SEQUENCE>
</SIMULATION>

